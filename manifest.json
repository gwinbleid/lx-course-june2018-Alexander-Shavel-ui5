{
  "sap.app": {
    "id": "leverx.app",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "applicationVersion": {
      "version": "1.0.0"
    }
  },
  "sap.ui": {
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    },
    "supportedThemes": [
      "sap_belize"
    ]
  },
  "sap.ui5": {
    "rootView": "leverx.app.view.App",
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "leverx.app.view",
        "controlId": "RootApp",
        "controlAggregation": "pages",
        "bypassed": {
          "target": [
            "notFound"
          ]
        },
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "FirstPage",
          "target": [
            "firstPage"
          ]
        },
        {
          "pattern": "order",
          "name": "SecondPage",
          "target": [
            "secondPage"
          ]
        },
        {
          "pattern": "product",
          "name": "ThirdPage",
          "target": [
            "thirdPage"
          ]
        }
      ],

      "targets": {
        "firstPage": {
          "viewName": "FirstPage",
          "viewId": "firstPage",
          "viewLevel": 1
        },

        "secondPage": {
          "viewName": "SecondPage",
          "viewId": "secondPage",
          "viewLevel": 2
        },

        "thirdPage": {
          "viewName": "ThirdPage",
          "viewId": "thirdPage",
          "viewLevel": 3
        },

        "notFound": {
          "viewName": "NotFound",
          "viewId": "notFound"
        }
      }
    },
    "dependencies": {
      "minUI5Version": "1.54.0",
      "libs": {
        "sap.m": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "leverx.app.i18n.i18n"
        }
      }
    }
  }
}